{
	"name": "df_gold_hechos_llamadas",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_registro",
						"type": "DatasetReference"
					},
					"name": "dimServicio"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_registro",
						"type": "DatasetReference"
					},
					"name": "dimRegistro"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "dimFecha"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_rbs",
						"type": "DatasetReference"
					},
					"name": "dimRbs"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_numero",
						"type": "DatasetReference"
					},
					"name": "dimNumero"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_gold_hechos_llamadas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "lookup4"
				},
				{
					"name": "lookup5"
				},
				{
					"name": "lookup6"
				},
				{
					"name": "select1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(10,7),",
				"          longitud_a as decimal(10,7),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(10,7),",
				"          longitud_b as decimal(10,7),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          id_servicio as integer,",
				"          servicio as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_servicio,servicio from gold.dim_servicio',",
				"     format: 'query') ~> dimServicio",
				"source(output(",
				"          id_registro as integer,",
				"          registro as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_registro,registro from gold.dim_registro',",
				"     format: 'query') ~> dimRegistro",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_fecha,fecha from gold.dim_fecha',",
				"     format: 'query') ~> dimFecha",
				"source(output(",
				"          id_radiobase as integer,",
				"          radiobase as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_radiobase, radiobase FROM gold.dim_radiobase',",
				"     format: 'query') ~> dimRbs",
				"source(output(",
				"          id_numero as integer,",
				"          numero as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_numero,numero from gold.dim_numero',",
				"     format: 'query') ~> dimNumero",
				"source1, dimServicio lookup(source1@servicio == dimServicio@servicio,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, dimRegistro lookup(source1@registro == dimRegistro@registro,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, dimFecha lookup(source1@fecha == dimFecha@fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup3, dimRbs lookup(rbs_utilizada_a == radiobase,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4",
				"select2, dimNumero lookup(numero_a == numero",
				"     && numero_b == numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup5",
				"select1, dimRbs lookup(rbs_utilizada_b == dimRbs@radiobase,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup6",
				"lookup4 select(mapColumn(",
				"          servicio = source1@servicio,",
				"          registro = source1@registro,",
				"          rbs_utilizada_a,",
				"          numero_b,",
				"          imei_b,",
				"          rbs_utilizada_b,",
				"          celda_b,",
				"          latitud_b,",
				"          longitud_b,",
				"          fecha = source1@fecha,",
				"          duracion,",
				"          id_servicio,",
				"          servicio = dimServicio@servicio,",
				"          id_registro,",
				"          registro = dimRegistro@registro,",
				"          id_fecha,",
				"          fecha = dimFecha@fecha,",
				"          id_radiobase,",
				"          radiobase",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"lookup6 select(mapColumn(",
				"          servicio,",
				"          registro,",
				"          rbs_utilizada_a,",
				"          rbs_utilizada_b,",
				"          fecha,",
				"          duracion,",
				"          id_servicio,",
				"          id_registro,",
				"          id_fecha,",
				"          id_radiobase = select1@id_radiobase,",
				"          radiobase = select1@radiobase,",
				"          id_radiobase = dimRbs@id_radiobase,",
				"          radiobase = dimRbs@radiobase",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"lookup5 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_llamada as integer,",
				"          duracion as timestamp,",
				"          id_servicio as integer,",
				"          id_registro as integer,",
				"          id_numera_a as integer,",
				"          id_radiobase_a as integer,",
				"          id_numero_b as integer,",
				"          id_radiobase_b as integer,",
				"          id_fecha as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}